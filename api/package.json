    "node": ">=18.0.0"
  },
  "dependencies": {
    "@x402/core": "2.2.0",
    "@x402/evm": "2.2.0",
    "@x402/express": "2.2.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
